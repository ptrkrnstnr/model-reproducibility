FROM gams:24.8.3-x64_64

# Set model 
ENV MODEL=OSCAR#

# Set model version
ENV MODEL_VERSION=v2.2

RUN wget -q -c --no-check-certificate https://github.com/vwmaus/${MODEL}/releases/${MODEL_VERSION}.zip -O /workspace/model.zip &&\
     cd /worspace &&\
     7z e /workspace/model.zip 

# Create docker volume 
VOLUME /workspace

# Run model 
RUN gams ${MODEL}-${MODEL_VERSION}/main.gms gdx=output


