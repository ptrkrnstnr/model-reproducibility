FROM gams:24.8.3-x64_64

# Set model 
ENV MODEL=transport-model

# Set model version
ENV MODEL_VERSION=0.0.0

RUN wget -q -c --no-check-certificate https://github.com/vwmaus/transport-model/releases/v2.0.zip -O /workspace/model.zip &&\
     cd /worspace &&\
     7z e /workspace/model.zip 

# Run model 
RUN gams transport-model-vv2.0/main.gms 


